412 void hitung_6gelang() 
413 { 
414     int G1=0, G2=0, G3=0, G4=0, G5=0, G6=0; 
415     int N=0; 
416     char WARNA[20]; 
417     char p= '%'; 
418 
 
419     for (N=1; N<=6; N++) 
420     { 
421         printf(" Masukkan warna GELANG ke %d : ", N); 
422         scanf("%s", WARNA); 
423         if(strcmp(WARNA, "hitam")==0) 
424         { 
425             switch(N) 
426             { 
427                 case 1: G1=0; break; 
428                 case 2: G2=0; break; 
429                 case 3: G3=0; break; 
430                 case 4: G4=pow(10,0); break; 
431                 case 5: G5=0; break; 
432                 case 6: G6=250; break; 
433             } 
434         } 
435         else if(strcmp(WARNA, "coklat")==0) 
436         { 
437             switch(N) 
438             { 
439                 case 1: G1=1; break; 
440                 case 2: G2=1; break; 
441                 case 3: G3=1; break; 
442                 case 4: G4=pow(10,1); break; 
443                 case 5: G5=1; break; 
444                 case 6: G6=100; break; 
445             } 
446         } 
447         else if(strcmp(WARNA, "merah")==0) 
448         { 
449             switch(N) 
450             { 
451                 case 1: G1=2; break; 
452                 case 2: G2=2; break; 
453                 case 3: G3=2; break; 
454                 case 4: G4=pow(10,2); break; 
455                 case 5: G5=2; break; 
456                 case 6: G6=50; break; 
457             } 
458         } 
459         else if(strcmp(WARNA, "orange")==0) 
460         { 
461             switch(N) 
462             { 
463                 case 1: G1=3; break; 
464                 case 2: G2=3; break; 
465                 case 3: G3=3; break; 
466                 case 4: G4=pow(10,3); break; 
467                 case 5: G5=0; break; 
468                 case 6: G6=15; break; 
469             } 
470         } 
471         else if(strcmp(WARNA, "kuning")==0) 
472         { 
473             switch(N) 
474             { 
475                 case 1: G1=4; break; 
476                 case 2: G2=4; break; 
477                 case 3: G3=4; break; 
478                 case 4: G4=pow(10,4); break; 
479                 case 5: G5=0; break; 
480                 case 6: G6=25; break; 
481             } 
482         } 
483          else if(strcmp(WARNA, "hijau")==0) 
484         { 
485             switch(N) 
486             { 
487                 case 1: G1=5; break; 
488                 case 2: G2=5; break; 
489                 case 3: G3=5; break; 
490                 case 4: G4=pow(10,5); break; 
491                 case 5: G5=0.5; break; 
492                 case 6: G6=20; break; 
493             } 
494         } 
495         else if(strcmp(WARNA, "biru")==0) 
496         { 
497             switch(N) 
498             { 
499                 case 1: G1=6; break; 
500                 case 2: G2=6; break; 
501                 case 3: G3=6; break; 
502                 case 4: G4=pow(10,6); break; 
503                 case 5: G5=0.25; break; 
504                 case 6: G6=10; break; 
505             } 
506         } 
507         else if(strcmp(WARNA, "ungu")==0) 
508         { 
509             switch(N) 
510             { 
511                 case 1: G1=7; break; 
512                 case 2: G2=7; break; 
513                 case 3: G3=7; break; 
514                 case 4: G4=pow(10,7); break; 
515                 case 5: G5=0.1; break; 
516                 case 6: G6=5; break; 
517             } 
518         } 
519         else if(strcmp(WARNA, "abu-abu")==0) 
520         { 
521             switch(N) 
522             { 
523                 case 1: G1=8; break; 
524                 case 2: G2=8; break; 
525                 case 3: G3=8; break; 
526                 case 4: G4=pow(10,8); break; 
527                 case 5: G5=0.05; break; 
528                 case 6: G6=1; break; 
529             } 
530         } 
531         else if(strcmp(WARNA, "putih")==0) 
532         { 
533             switch(N) 
534             { 
535                 case 1: G1=9; break; 
536                 case 2: G2=9; break; 
537                 case 3: G3=9; break; 
538                 case 4: G4=pow(10,9); break; 
539                 case 5: G5=0; break; 
540                 case 6: G6=0; break; 
541             } 
542         } 
543          else if(strcmp(WARNA, "emas")==0) 
544         { 
545             switch(N) 
546             { 
547                case 4: G4=pow(10,-1); break; 
548                case 5: G5=5; break; 
549             } 
550         } 
551          else if(strcmp(WARNA, "perak")==0) 
552         { 
553             switch(N) 
554             { 
555                case 4: G4=pow(10,-2); break; 
556                case 5: G5=10; break; 
557             } 
558         } 
559         else if(strcmp(WARNA, "tak berwarna")==0) 
560         { 
561             switch(N) 
562             { 
563                case 5: G5=20; break; 
564             } 
565         } 
566     } 
567     N=((((G1*100)+(G2*10))+G3)*G4); 
568     printf("\nNilai resistor %d ohm dengan toleransi %d%c %d ppm/K", N,G5,p,G6); 
569 } 
570 
 
571 int main() 
572 { 
573     int GELANG = 0; 
574     while((GELANG<3)||(GELANG>6)) 
575     { 
576         printf("Masukkan jumlah gelang : "); 
577         scanf("%d",&GELANG); 
578         if((GELANG<3) || (GELANG>6)) 
579         { 
580             printf("\n==========================="); 
581             printf("\nNilai GELANG antara 3 s/d 6"); 
582             printf("/n===========================\n"); 
583         } 
584         if(GELANG==3) 
585         { 
586             hitung_3gelang(); 
587         } 
588         else if(GELANG==4) 
589         { 
590             hitung_4gelang(); 
591         } 
592         else if(GELANG==5) 
593         { 
594             hitung_5gelang(); 
595         } 
596         else if(GELANG==6) 
597         { 
598             hitung_6gelang(); 
599         } 
600     } 
601     return 0; 
602 } 
