263 void hitung_5gelang() 
264 { 
265     int G1=0, G2=0, G3=0, G4=0, G5=0; 
266     int N=0; 
267     char WARNA[20]; 
268     char p= '%'; 
269 
 
270     for (N=1; N<=5; N++) 
271     { 
272         printf(" Masukkan warna GELANG ke %d : ", N); 
273         scanf("%s", WARNA); 
274         if(strcmp(WARNA, "hitam")==0) 
275         { 
276             switch(N) 
277             { 
278                 case 1: G1=0; break; 
279                 case 2: G2=0; break; 
280                 case 3: G3=0; break; 
281                 case 4: G4=pow(10,0); break; 
282                 case 5: G5=1; break; 
283             } 
284         } 
285         else if(strcmp(WARNA, "coklat")==0) 
286         { 
287             switch(N) 
288             { 
289                 case 1: G1=1; break; 
290                 case 2: G2=1; break; 
291                 case 3: G3=1; break; 
292                 case 4: G4=pow(10,1); break; 
293                 case 5: G5=2; break; 
294             } 
295         } 
296         else if(strcmp(WARNA, "merah")==0) 
297         { 
298             switch(N) 
299             { 
300                 case 1: G1=2; break; 
301                 case 2: G2=2; break; 
302                 case 3: G3=2; break; 
303                 case 4: G4=pow(10,2); break; 
304                 case 5: G5=3; break; 
305             } 
306         } 
307         else if(strcmp(WARNA, "orange")==0) 
308         { 
309             switch(N) 
310             { 
311                 case 1: G1=3; break; 
312                 case 2: G2=3; break; 
313                 case 3: G3=3; break; 
314                 case 4: G4=pow(10,3); break; 
315                 case 5: G5=4; break; 
316             } 
317         } 
318         else if(strcmp(WARNA, "kuning")==0) 
319         { 
320             switch(N) 
321             { 
322                 case 1: G1=4; break; 
323                 case 2: G2=4; break; 
324                 case 3: G3=4; break; 
325                 case 4: G4=pow(10,4); break; 
326                 case 5: G5=0; break; 
327             } 
328         } 
329          else if(strcmp(WARNA, "hijau")==0) 
330         { 
331             switch(N) 
332             { 
333                 case 1: G1=5; break; 
334                 case 2: G2=5; break; 
335                 case 3: G3=5; break; 
336                 case 4: G4=pow(10,5); break; 
337                 case 5: G5=0.5; break; 
338             } 
339         } 
340         else if(strcmp(WARNA, "biru")==0) 
341         { 
342             switch(N) 
343             { 
344                 case 1: G1=6; break; 
345                 case 2: G2=6; break; 
346                 case 3: G3=6; break; 
347                 case 4: G4=pow(10,6); break; 
348                 case 5: G5=0.25; break; 
349             } 
350         } 
351         else if(strcmp(WARNA, "ungu")==0) 
352         { 
353             switch(N) 
354             { 
355                 case 1: G1=7; break; 
356                 case 2: G2=7; break; 
357                 case 3: G3=7; break; 
358                 case 4: G4=pow(10,7); break; 
359                 case 5: G5=0.1; break; 
360             } 
361         } 
362         else if(strcmp(WARNA, "abu-abu")==0) 
363         { 
364             switch(N) 
365             { 
366                 case 1: G1=8; break; 
367                 case 2: G2=8; break; 
368                 case 3: G3=8; break; 
369                 case 4: G4=pow(10,8); break; 
370                 case 5: G5=0.05; break; 
371             } 
372         } 
373         else if(strcmp(WARNA, "putih")==0) 
374         { 
375             switch(N) 
376             { 
377                 case 1: G1=9; break; 
378                 case 2: G2=9; break; 
379                 case 3: G3=9; break; 
380                 case 4: G4=pow(10,9); break; 
381                 case 5: G5=0; break; 
382             } 
383         } 
384          else if(strcmp(WARNA, "emas")==0) 
385         { 
386             switch(N) 
387             { 
388                case 4: G4=pow(10,-1); break; 
389                case 5: G5=5; break; 
390             } 
391         } 
392          else if(strcmp(WARNA, "perak")==0) 
393         { 
394             switch(N) 
395             { 
396                case 4: G4=pow(10,-2); break; 
397                case 5: G5=10; break; 
398             } 
399         } 
400         else if(strcmp(WARNA, "tak berwarna")==0) 
401         { 
402             switch(N) 
403             { 
404                case 5: G5=20; break; 
405             } 
406         } 
407     } 
408     N=(((G1*100)+(G2*10))+G3)*G4; 
409     printf("\nNilai resistor %d ohm dengan toleransi %d%c", N, G5, p); 
410 } 
