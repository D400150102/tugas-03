124 void hitung_4gelang() 
125 { 
126     int G1=0, G2=0, G3=0, G4=0; 
127     int N=0; 
128     char WARNA[20]; 
129     char p= '%'; 
130 
 
131     for (N=1; N<=4; N++) 
132     { 
133         printf(" Masukkan warna GELANG ke %d : ", N); 
134         scanf("%s", WARNA); 
135         if(strcmp(WARNA, "hitam")==0) 
136         { 
137             switch(N) 
138             { 
139                 case 1: G1=0; break; 
140                 case 2: G2=0; break; 
141                 case 3: G3=pow(10,0); break; 
142                 case 4: G4=0; break; 
143             } 
144         } 
145         else if(strcmp(WARNA, "coklat")==0) 
146         { 
147             switch(N) 
148             { 
149                 case 1: G1=1; break; 
150                 case 2: G2=1; break; 
151                 case 3: G3=pow(10,1); break; 
152                 case 4: G4=1; break; 
153             } 
154         } 
155         else if(strcmp(WARNA, "merah")==0) 
156         { 
157             switch(N) 
158             { 
159                 case 1: G1=2; break; 
160                 case 2: G2=2; break; 
161                 case 3: G3=pow(10,2); break; 
162                 case 4: G4=2; break; 
163             } 
164         } 
165         else if(strcmp(WARNA, "orange")==0) 
166         { 
167             switch(N) 
168             { 
169                 case 1: G1=3; break; 
170                 case 2: G2=3; break; 
171                 case 3: G3=pow(10,3); break; 
172                 case 4: G4=0; break; 
173             } 
174         } 
175         else if(strcmp(WARNA, "kuning")==0) 
176         { 
177             switch(N) 
178             { 
179                 case 1: G1=4; break; 
180                 case 2: G2=4; break; 
181                 case 3: G3=pow(10,4); break; 
182                 case 4: G4=0; break; 
183             } 
184         } 
185          else if(strcmp(WARNA, "hijau")==0) 
186         { 
187             switch(N) 
188             { 
189                 case 1: G1=5; break; 
190                 case 2: G2=5; break; 
191                 case 3: G3=pow(10,5); break; 
192                 case 4: G4=0.5; break; 
193             } 
194         } 
195         else if(strcmp(WARNA, "biru")==0) 
196         { 
197             switch(N) 
198             { 
199                 case 1: G1=6; break; 
200                 case 2: G2=6; break; 
201                 case 3: G3=pow(10,6); break; 
202                 case 4: G4=0.25; break; 
203             } 
204         } 
205         else if(strcmp(WARNA, "ungu")==0) 
206         { 
207             switch(N) 
208             { 
209                 case 1: G1=7; break; 
210                 case 2: G2=7; break; 
211                 case 3: G3=pow(10,7); break; 
212                 case 4: G4=0.1; break; 
213             } 
214         } 
215         else if(strcmp(WARNA, "abu-abu")==0) 
216         { 
217             switch(N) 
218             { 
219                 case 1: G1=8; break; 
220                 case 2: G2=8; break; 
221                 case 3: G3=pow(10,8); break; 
222                 case 4: G4=0.05; break; 
223             } 
224         } 
225         else if(strcmp(WARNA, "putih")==0) 
226         { 
227             switch(N) 
228             { 
229                 case 1: G1=9; break; 
230                 case 2: G2=9; break; 
231                 case 3: G3=pow(10,9); break; 
232                 case 4: G4=0; break; 
233             } 
234         } 
235          else if(strcmp(WARNA, "emas")==0) 
236         { 
237             switch(N) 
238             { 
239                case 3: G3=pow(10,-1); break; 
240                case 4: G4=5; break; 
241             } 
242         } 
243          else if(strcmp(WARNA, "perak")==0) 
244         { 
245             switch(N) 
246             { 
247                case 3: G3=pow(10,-2); break; 
248                case 4: G4=10; break; 
249             } 
250         } 
251         else if(strcmp(WARNA, "tak berwarna")==0) 
252         { 
253             switch(N) 
254             { 
255                case 4: G4=20; break; 
256             } 
257         } 
258     } 
259     N=((G1*10)+G2)*G3; 
260     printf("\nNilai resistor %d ohm dengan toleransi %d%c", N, G4, p); 
261 } 
