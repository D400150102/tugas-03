 #include<stdio.h> 
2 #include<string.h> 
3 #include<math.h> 
4 
 
5 void hitung_3gelang() 
6 { 
7     int G1=0, G2=0, G3=0; 
8     int N=0; 
9     char WARNA[20]; 
10 
 
11     for (N=1; N<=3; N++) 
12     { 
13         printf(" Masukkan warna GELANG ke %d : ", N); 
14         scanf("%s", WARNA); 
15         if(strcmp(WARNA, "hitam")==0) 
16         { 
17             switch(N) 
18             { 
19                 case 1: G1=0; break; 
20                 case 2: G2=0; break; 
21                 case 3: G3=pow(10,0); break; 
22             } 
23         } 
24         else if(strcmp(WARNA, "coklat")==0) 
25         { 
26             switch(N) 
27             { 
28                 case 1: G1=1; break; 
29                 case 2: G2=1; break; 
30                 case 3: G3=pow(10,1); break; 
31             } 
32         } 
33         else if(strcmp(WARNA, "merah")==0) 
34         { 
35             switch(N) 
36             { 
37                 case 1: G1=2; break; 
38                 case 2: G2=2; break; 
39                 case 3: G3=pow(10,2); break; 
40             } 
41         } 
42         else if(strcmp(WARNA, "orange")==0) 
43         { 
44             switch(N) 
45             { 
46                 case 1: G1=3; break; 
47                 case 2: G2=3; break; 
48                 case 3: G3=pow(10,3); break; 
49             } 
50         } 
51         else if(strcmp(WARNA, "kuning")==0) 
52         { 
53             switch(N) 
54             { 
55                 case 1: G1=4; break; 
56                 case 2: G2=4; break; 
57                 case 3: G3=pow(10,4); break; 
58             } 
59         } 
60          else if(strcmp(WARNA, "hijau")==0) 
61         { 
62             switch(N) 
63             { 
64                 case 1: G1=5; break; 
65                 case 2: G2=5; break; 
66                 case 3: G3=pow(10,5); break; 
67             } 
68         } 
69         else if(strcmp(WARNA, "biru")==0) 
70         { 
71             switch(N) 
72             { 
73                 case 1: G1=6; break; 
74                 case 2: G2=6; break; 
75                 case 3: G3=pow(10,6); break; 
76             } 
77         } 
78         else if(strcmp(WARNA, "ungu")==0) 
79         { 
80             switch(N) 
81             { 
82                 case 1: G1=7; break; 
83                 case 2: G2=7; break; 
84                 case 3: G3=pow(10,7); break; 
85             } 
86         } 
87         else if(strcmp(WARNA, "abu-abu")==0) 
88         { 
89             switch(N) 
90             { 
91                 case 1: G1= 8; break; 
92                 case 2: G2=8; break; 
93                 case 3: G3=pow(10,8); break; 
94             } 
95         } 
96         else if(strcmp(WARNA, "putih")==0) 
97         { 
98             switch(N) 
99             { 
100                 case 1: G1=9; break; 
101                 case 2: G2=9; break; 
102                 case 3: G3=pow(10,9); break; 
103             } 
104         } 
105          else if(strcmp(WARNA, "emas")==0) 
106         { 
107             switch(N) 
108             { 
109                case 3: G3=pow(10,-1); break; 
110             } 
111         } 
112          else if(strcmp(WARNA, "perak")==0) 
113         { 
114             switch(N) 
115             { 
116                case 3: G3=pow(10,-2); break; 
117             } 
118         } 
119     } 
120     N=((G1*10)+G2)*G3; 
121     printf("\nNilai resistor R = %d ohm", N); 
122 } 
